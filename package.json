{
  "name": "my-office-manager-new",
  "version": "1.1.1",
  "description": "A the office package",
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "module": "./lib/esm/index.js",
  "bin": {
    "my-script": "./bin/my-script"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lisilinhart/the-office-manager-1234.git"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "start": "npm run build && npm run global",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "global": "npm i -g && npx my-script",
    "test": "jest --coverage",
    "clean": "rm -rf lib",
    "build": "npm run clean && npx tsc -p tsconfig.json && npx tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "the office",
    "generator",
    "scaffolding"
  ],
  "author": "Lisi Linhart <ewl@storyblok.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "husky": "^8.0.0",
    "i": "^0.3.7",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "npm": "^8.9.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^9.3.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "path": "^0.12.7"
  }
}